{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'QuestionsList',\n  data() {\n    return {\n      questions: []\n    };\n  },\n  created() {\n    this.fetchQuestions();\n  },\n  methods: {\n    fetchQuestions() {\n      axios.get('http://localhost:8081/api/v1/questions').then(response => {\n        this.questions = response.data;\n      }).catch(error => {\n        console.error('获取问题列表失败:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","questions","created","fetchQuestions","methods","get","then","response","catch","error","console"],"sources":["src/components/QuestionsList.vue"],"sourcesContent":["<template>\n    <div>\n      <h2>问题列表</h2>\n      <ul>\n        <li v-for=\"question in questions\" :key=\"question.id\">\n          {{ question.title }}\n          <router-link :to=\"'/question/edit/' + question.id\">编辑</router-link>\n        </li>\n      </ul>\n      <router-link to=\"/question/add\">添加新问题</router-link>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  \n  export default {\n    name: 'QuestionsList',\n    data() {\n      return {\n        questions: [],\n      };\n    },\n    created() {\n      this.fetchQuestions();\n    },\n    methods: {\n      fetchQuestions() {\n        axios.get('http://localhost:8081/api/v1/questions')\n\n          .then(response => {\n            this.questions = response.data;\n          })\n          .catch(error => {\n            console.error('获取问题列表失败:', error);\n          });\n      }\n    }\n  };\n  </script>\n  "],"mappings":"AAcA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,cAAA;EACA;EACAC,OAAA;IACAD,eAAA;MACAL,KAAA,CAAAO,GAAA,2CAEAC,IAAA,CAAAC,QAAA;QACA,KAAAN,SAAA,GAAAM,QAAA,CAAAP,IAAA;MACA,GACAQ,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,cAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}