{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'QuestionsList',\n  data() {\n    return {\n      questions: []\n    };\n  },\n  created() {\n    this.fetchQuestions();\n  },\n  methods: {\n    fetchQuestions() {\n      axios.get('http://localhost:8080/api/v1/questions').then(response => {\n        this.questions = response.data;\n      }).catch(error => {\n        console.error('获取问题列表失败:', error);\n      });\n    },\n    deleteQuestion(id) {\n      axios.delete(`http://localhost:8080/api/v1/questions/${id}`).then(() => {\n        this.fetchQuestions();\n      }).catch(error => {\n        console.error('fail to delete question:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","questions","created","fetchQuestions","methods","get","then","response","catch","error","console","deleteQuestion","id","delete"],"sources":["src/components/QuestionsList.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>List de Questions</h2>\n    <ul class=\"list-group\">\n      <li v-for=\"question in questions\" :key=\"question.id\" class=\"list-group-item\">\n        {{ question.title }}\n        <router-link :to=\"'/question/edit/' + question.id\" class=\"btn btn-primary btn-sm\">Edit</router-link>\n        <button @click=\"deleteQuestion(question.id)\" class=\"btn btn-danger btn-sm\">Delete</button>\n      </li>\n    </ul>\n    <router-link to=\"/question/add\" class=\"btn btn-success\">New questiones</router-link>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'QuestionsList',\n  data() {\n    return {\n      questions: [],\n    };\n  },\n  created() {\n    this.fetchQuestions();\n  },\n  methods: {\n    fetchQuestions() {\n      axios.get('http://localhost:8080/api/v1/questions')\n        .then(response => {\n          this.questions = response.data;\n        })\n        .catch(error => {\n          console.error('获取问题列表失败:', error);\n        });\n    },\n    deleteQuestion(id) {\n      axios.delete(`http://localhost:8080/api/v1/questions/${id}`)\n        .then(() => {\n          this.fetchQuestions(); \n        })\n        .catch(error => {\n          console.error('fail to delete question:', error);\n        });\n    }\n  }\n};\n</script>\n\n<style>\n.container {\n  padding: 20px;\n  background-color: #f9f9f9;\n  border-radius: 5px;\n  box-shadow: 0 0 10px rgba(0,0,0,0.1);\n}\n\n.list-group {\n  margin-bottom: 20px;\n}\n\n.list-group-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.btn {\n  margin-left: 10px;\n}\n</style>\n"],"mappings":"AAeA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,cAAA;EACA;EACAC,OAAA;IACAD,eAAA;MACAL,KAAA,CAAAO,GAAA,2CACAC,IAAA,CAAAC,QAAA;QACA,KAAAN,SAAA,GAAAM,QAAA,CAAAP,IAAA;MACA,GACAQ,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,cAAAA,KAAA;MACA;IACA;IACAE,eAAAC,EAAA;MACAd,KAAA,CAAAe,MAAA,2CAAAD,EAAA,IACAN,IAAA;QACA,KAAAH,cAAA;MACA,GACAK,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,6BAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}